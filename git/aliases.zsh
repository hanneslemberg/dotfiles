alias glog="git log --follow --graph --pretty=format:'%Cred%h%Creset %an: %s - %Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative"
alias gd='git diff -w'
alias gc='git commit'
alias gco='git checkout'
alias gb='git branch'
alias gs='git status -sb' 
alias git-ignore-local='git update-index --assume-unchanged'


